<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.systemMsg.sysMsg.mapper.SysMsgMapper">

	<resultMap id="sysMsgMapper" type="com.example.systemMsg.sysMsg.enity.SysMsg">
		<result property="ids" column="ids" />
		<result property="name" column="name" />
		<result property="isAdmin" column="isAdmin" />
		<result property="isFile" column="isFile" />
		<result property="hasFile" column="hasFile" />
		<result property="isEmail" column="isEmail" />
		<result property="hasEmail" column="hasEmail" />
		<result property="isblos" column="isblos" />
		<result property="hasBlos" column="hasBlos" />
		<result property="isService" column="isService" />
		<result property="hasService" column="hasService" />
	</resultMap>

	<select id="selectSysMsg" resultMap="sysMsgMapper">

	</select>
	
</mapper>